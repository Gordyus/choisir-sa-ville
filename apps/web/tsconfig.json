{
  "extends": "../../tsconfig.base.json",
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "./dist/out-tsc",
    "target": "ES2022",
    "module": "ES2022",
    "moduleResolution": "Bundler",
    "noEmit": false,
    "useDefineForClassFields": false,
    "strict": true,
    "lib": ["ES2022", "DOM"],
    "skipLibCheck": true,
    "types": []
  },
  "angularCompilerOptions": {
    "strictTemplates": true
  }
}
