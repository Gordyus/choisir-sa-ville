{
  "name": "@choisir-sa-ville/api",
  "version": "1.0.0",
  "type": "module",
  "description": "Backend routing service for travel time calculations",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:unit": "vitest run __tests__/routing/unit __tests__/health/unit",
    "test:integration": "vitest run __tests__/routing/integration __tests__/health/integration",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "fastify": "^5.1.0",
    "@fastify/cors": "^10.0.1",
    "@fastify/rate-limit": "^10.1.1",
    "ngeohash": "^0.6.3",
    "p-retry": "^6.2.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  }
}
