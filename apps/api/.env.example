# Provider (choose one: navitia | tomtom | mock)
# - navitia: SNCF API, 150k req/month free, excellent for France
# - tomtom: Global coverage, 75k req/month free
# - mock: Development/testing, no external calls
ROUTING_PROVIDER=navitia

# API Keys (provide key for your chosen provider)
NAVITIA_API_KEY=c8717cea-44a5-429d-aa9c-27184ecd8d6e
TOMTOM_API_KEY=

# Cache
ENABLE_CACHE=false
DATABASE_URL=

# Optimizations
GEOHASH_PRECISION=6
TIME_BUCKET_MINUTES=30
CACHE_TTL_DAYS=30
TRAVEL_TIME_MARGIN_PERCENT=10

# Rate limiting
RATE_LIMIT_REQUESTS_PER_WINDOW=1000
RATE_LIMIT_WINDOW_MS=60000

# Server
PORT=3001
CORS_ORIGIN=http://localhost:3000
NODE_ENV=development

# Monitoring (optional)
SENTRY_DSN=
